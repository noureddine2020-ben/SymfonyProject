<div class="card md-4">
    {% if property.filename %}
        <img src="/images/properties/{{ property.filename }}" alt="card-img-top" style="width: 100%;height: auto;">
    {% endif %}
    <div class="card-body">
        <h5 class="card-title">
            <a href="{{ path('property.show',{id:property.id}) }}"> {{ property.title }} </a>

        </h5>
        <p class="card-text"> {{ property.location }}</p>
        <div class="text-primary" style="font-size: 2rem;font-weight: bolder"> {{ property.FormattedPrice() }} DT</div>

    </div>

</div>